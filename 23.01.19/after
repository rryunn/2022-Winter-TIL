#include <iostream>

int main(){
    int val1, val2, val3, val4, val5;
    int result = 0;

    std::cout<<" 첫 번째 정수 입력: ";
    std::cin >> val1;
    std::cout << "두 번째 정수 입력: ";
    std::cin >> val2;
    std::cout<<" 세 번째 정수 입력: ";
    std::cin >> val3;
    std::cout << "네 번째 정수 입력: ";
    std::cin >> val4;
    std::cout << "다섯 번째 정수 입력 : ";
    std::cin >> val5;

    result = val1 + val2 + val3 + val4 + val5;
    std::cout<<result;
  }
