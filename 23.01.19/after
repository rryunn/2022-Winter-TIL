  #include <iostream>
  
  int main(){
    char name[100];
    char numb[200];
    
    std::cout<<" 이름을 입력하시오" ;
    std::cin >> name;
    std::cout <<"전화번호를 입력하시오:";
    std::cin>> numb;
    
    std::cout<< name <<numb;
    
    }
